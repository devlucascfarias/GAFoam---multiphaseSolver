FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0";
    object      alpha.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [];

internalField   uniform 1;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    outlet
    {
        type            zeroGradient;
    }
    inlet
    {
        type            inletOutlet;
        inletValue      nonuniform List<scalar> 
0
;
        value           nonuniform List<scalar> 
0
;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary0to1
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary0to3
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary0to4
    {
        type            processor;
        value           uniform 1;
    }
    procBoundary0to5
    {
        type            processor;
        value           uniform 1;
    }
}



FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0.49";
    object      T.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    walls
    {
        type            zeroGradient;
    }
    bottom
    {
        type            zeroGradient;
    }
    outlet
    {
        type            inletOutlet;
        phi             phi.water;
        inletValue      uniform 300;
        value           uniform 300;
    }
    inlet
    {
        type            inletOutlet;
        phi             phi.water;
        inletValue      uniform 300;
        value           uniform 300;
    }
    defaultFaces
    {
        type            empty;
    }
}



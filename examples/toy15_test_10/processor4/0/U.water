FoamFile
{
    format      binary;
    class       volVectorField;
    location    "0";
    object      U.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    walls
    {
        type            noSlip;
    }
    bottom
    {
        type            noSlip;
    }
    outlet
    {
        type            pressureInletOutletVelocity;
        phi             phi.water;
        value           nonuniform List<vector> 
0
;
    }
    inlet
    {
        type            fixedValue;
        value           nonuniform List<vector> 
0
;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary4to0
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary4to3
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary4to5
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary4to6
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary4to11
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary4to14
    {
        type            processor;
        value           uniform (0 0 0);
    }
    procBoundary4to15
    {
        type            processor;
        value           uniform (0 0 0);
    }
}



FoamFile
{
    format      ascii;
    class       volScalarField;
    object      k.water;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 2 -2 0 0 0 0];

internalField       uniform 1e-7;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value uniform 	 0.00375;
    }

    outlet
    {
        type            inletOutlet;
        phi             phi.water;
        inletValue      $internalField;
        value           $internalField;
    }

    walls
    {
        type            kqRWallFunction;
        value           $internalField;
    }
    
    bottom
    {
        type            kqRWallFunction;
        value           $internalField;
    }

    frontAndBackPlanes
    {
        type            empty;
    }
}


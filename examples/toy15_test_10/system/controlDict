FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     foamRun;

solver          multiphaseEuler;

startFrom       latestTime;

startTime       0;

stopAt          endTime;

endTime         1;

deltaT          0.001;

writeControl    adjustableRunTime;

writeInterval   0.01;

purgeWrite      0;

writeFormat     binary;

writePrecision  6;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable on;

adjustTimeStep  yes;

maxCo           0.5;        

maxAlphaCo      0.2;         

maxDeltaT       0.05;





InfoSwitches
{
    time 1;
}

DebugSwitches
{
    // 0 = off, 1 = on
    InfoSwitch          1;
    TimeRegistry        1;  // Esta Ã© a chave! Garante profiling detalhado.
}






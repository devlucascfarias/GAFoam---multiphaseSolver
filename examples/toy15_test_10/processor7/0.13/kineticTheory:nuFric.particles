FoamFile
{
    format      binary;
    class       volScalarField;
    location    "0.13";
    object      kineticTheory:nuFric.particles;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    walls
    {
        type            calculated;
        value           uniform 0;
    }
    bottom
    {
        type            calculated;
        value           uniform 0;
    }
    outlet
    {
        type            calculated;
        value           nonuniform List<scalar> 
0
;
    }
    inlet
    {
        type            calculated;
        value           nonuniform List<scalar> 
0
;
    }
    defaultFaces
    {
        type            empty;
    }
    procBoundary7to5
    {
        type            processor;
        value           uniform 0;
    }
    procBoundary7to6
    {
        type            processor;
        value           uniform 0;
    }
}


